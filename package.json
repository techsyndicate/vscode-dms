{
	"name": "vscode-dms",
	"displayName": "Direct Messages",
	"icon": "media/logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/techsyndicate/vscode-dms"
	},
	"description": "Direct Messages for VS Code",
	"publisher": "techsyndicate",
	"version": "0.9.0",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onView:dm-full"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"menus": {
			"view/title": [
				{
					"command": "vscode-dms.info",
					"when": "view == dm-full",
					"group": "navigation"
				},
				{
					"command": "vscode-dms.settings",
					"when": "view == dm-full",
					"group": "navigation"
				}
			],
			"editor/context": [
				{
					"command": "vscode-dms.sendMessage"
				}
			]
		},
		"commands": [
			{
				"command": "vscode-dms.sendMessage",
				"title": "Send Code"
			},
			{
				"command": "vscode-dms.info",
				"title": "Information",
				"icon": "media/white-info.svg"
			},
			{
				"command": "vscode-dms.settings",
				"title": "Settings",
				"icon": "media/settings.png"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "vscode-dms",
					"title": "Direct Messages",
					"icon": "media/inbox.svg"
				}
			]
		},
		"views": {
			"vscode-dms": [
				{
					"type": "webview",
					"id": "dm-full",
					"name": "Direct Messages",
					"icon": "media/inbox.svg",
					"contextualTitle": "Direct Messages"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack --devtool nosources-source-map --config ./build/node-extension.webpack.config.js",
		"watch": "webpack --watch --devtool nosources-source-map --info-verbosity verbose --config ./build/node-extension.webpack.config.js",
		"package": "webpack --mode production --config ./build/node-extension.webpack.config.js",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"svelte-build": "rollup -c",
		"svelte-dev": "rollup -c -w",
		"test-svelte": "jest"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^16.0.0",
		"@rollup/plugin-node-resolve": "^10.0.0",
		"@rollup/plugin-replace": "^2.3.4",
		"@rollup/plugin-typescript": "^6.1.0",
		"@tsconfig/svelte": "^1.0.10",
		"@types/form-data": "^2.5.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.19.8",
		"@types/vscode": "^1.51.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"rollup": "^2.33.2",
		"rollup-plugin-livereload": "^2.0.0",
		"rollup-plugin-svelte": "^6.1.1",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.29.7",
		"svelte-check": "^1.1.12",
		"svelte-preprocess": "^4.6.0",
		"ts-loader": "^8.0.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"@types/jest": "^26.0.15",
		"@types/node-fetch": "^2.5.7",
		"@types/polka": "^0.5.1",
		"@types/socket.io-client": "^1.4.34",
		"axios": "^0.21.1",
		"dotenv": "^8.2.0",
		"firebase": "^8.1.1",
		"jest": "^26.6.3",
		"node-fetch": "^2.6.1",
		"polka": "^0.5.2",
		"socket.io-client": "^3.0.2"
	}
}
